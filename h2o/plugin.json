{
    "id": "h2o",
    "version": "0.0.5",
    "meta": {
        "label": "H2O Machine Learning",
        "description": "Use H2O distributed ML library",
        "author": "Dataiku (Jean-Baptiste Rouquier)",
        "icon": "icon-tint",
        "licenseInfo": "Apache Software License",
        "url": "https://www.dataiku.com/dss/plugins/info/h2o.html",
        "tags": ["Machine Learning"]
    },
    "params": [
        {
            "name": "h2o_server_ip",
            "label": "H2O server host",
            "type": "STRING",
            "defaultValue": "localhost"
        },
        {
            "name": "h2o_server_port",
            "label": "H2O server port",
            "type": "INT",
            "defaultValue": 54321
        },
        {
            "name": "hdfs_models_path",
            "label": "HDFS path to store models",
            "type": "STRING",
            "description": "Only needed when H2O is running on Hadoop. E.g. /user/dataiku/dss_h2o_connector/models/",
            "mandatory": true
        }
    ]
}
